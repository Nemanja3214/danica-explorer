<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="app.Views.AccomodationCreateForm"
    x:DataType="viewModels:AccomodationCreateFormViewModel"
    xmlns="https://github.com/avaloniaui"
    xmlns:controls="https://github.com/AvaloniaUI/Avalonia.Samples/tree/main/src/Avalonia.Samples/CustomControls/RatingControlSample/RatingControlSample.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:app.ViewModels"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel Margin="10">
        <TextBox Margin="10" Text="Accomodationname" />
        <viewModels:RatingControl NumberOfStars="5" Value="{Binding RatingValue}" />
        <TextBox Margin="10" Text="Location" />
        <Button
            Command="{Binding Command}"
            Content="Location"
            Margin="10" />
        <TextBox
            AcceptsReturn="True"
            Height="100"
            Margin="10"
            Name="tbMultiLine"
            Text="Description"
            TextWrapping="Wrap" />
    </StackPanel>
</UserControl>
