    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Border Padding="30">
        <StackPanel>
            <TextBlock HorizontalAlignment="Center" Text="Day x" />
            <TextBlock HorizontalAlignment="Center" Text="Departure at x" />
            <TextBlock HorizontalAlignment="Center" Text="Check in in x" />
            <TextBlock HorizontalAlignment="Center" Text="Attraction x" />
            <TextBlock HorizontalAlignment="Center" Text="Attraction x" />
        </StackPanel>
    </Border>

</UserControl>