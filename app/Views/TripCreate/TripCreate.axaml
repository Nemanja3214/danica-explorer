<UserControl
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="app.Views.TripCreate"
    x:DataType="viewModels:TripCreateViewModel"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:app.ViewModels;assembly=app"
    xmlns:views="clr-namespace:app.Views;assembly=app"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel>
        <views:DragNDrop DataContext="{Binding AttractionVm}" />
        <views:DragNDrop DataContext="{Binding HotelVm}" />
        <views:DragNDrop DataContext="{Binding RestaurantVm}" />
    </StackPanel>
</UserControl>
